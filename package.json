{
  "name": "with-relay-modern",
  "version": "1.0.0",
  "description": "Example of Next.js with Relay Modern",
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "postinstall": "yarn deduplicate && patch-package",
    "deduplicate": "yarn yarn-deduplicate",
    "next": "next",
    "dev": "mkdir -p src/__generated__ && touch queryMap.json && node --require esbuild-register ./schema/printSchema.ts && concurrently \"next\" \"relay-compiler --watch\"",
    "build": "next build",
    "start": "next start",
    "relay": "relay-compiler",
    "schema": "node --require esbuild-register ./schema/printSchema.ts",
    "lint:eslint": "eslint --ext .ts,tsx .",
    "lint:prettier": "prettier . --check",
    "lint:style": "stylelint **/*.{css,scss}",
    "lint": "npm-run-all lint:*",
    "typecheck": "tsc -b",
    "format:eslint": "yarn lint:eslint --fix",
    "format:prettier": "prettier --write .",
    "format:style": "yarn lint:style --fix",
    "format": "npm-run-all format:*"
  },
  "license": "MIT",
  "dependencies": {
    "chance": "1.1.8",
    "graphql": "16.5.0",
    "next": "12.1.6",
    "nextjs-cors": "2.1.1",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-relay": "0.0.0-main-0a30fd6b",
    "relay-runtime": "0.0.0-main-0a30fd6b"
  },
  "devDependencies": {
    "@types/chance": "1.1.3",
    "@types/node": "17.0.31",
    "@types/react": "18.0.9",
    "@types/react-relay": "13.0.2",
    "@types/relay-runtime": "13.0.3",
    "@typescript-eslint/eslint-plugin": "5.23.0",
    "@typescript-eslint/parser": "5.23.0",
    "autoprefixer": "10.4.7",
    "concurrently": "7.1.0",
    "esbuild": "0.14.38",
    "esbuild-register": "3.3.2",
    "eslint": "8.15.0",
    "eslint-config-next": "12.1.6",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-typescript": "2.7.1",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "26.1.5",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.29.4",
    "eslint-plugin-testing-library": "5.4.0",
    "eslint-plugin-typescript-sort-keys": "2.1.0",
    "npm-run-all": "4.1.5",
    "patch-package": "6.4.7",
    "postcss": "8.4.13",
    "prettier": "2.6.2",
    "prettier-plugin-tailwindcss": "0.1.10",
    "relay-compiler": "0.0.0-main-9ff377ef",
    "stylelint": "14.8.2",
    "stylelint-config-prettier": "9.0.3",
    "stylelint-config-recommended": "7.0.0",
    "stylelint-config-standard": "25.0.0",
    "tailwindcss": "3.0.24",
    "typescript": "4.6.4",
    "yarn-deduplicate": "5.0.0"
  }
}
