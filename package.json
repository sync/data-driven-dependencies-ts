{
  "name": "with-relay-modern",
  "version": "1.0.0",
  "description": "Example of Next.js with Relay Modern",
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "next": "next",
    "dev": "mkdir -p __generated__ && touch queryMap.json && node ./schema/printSchema.mjs && concurrently \"next\" \"relay-compiler --watch\"",
    "build": "next build",
    "start": "next start",
    "relay": "relay-compiler",
    "schema": "node ./schema/printSchema.mjs",
    "prettier": "prettier --write '{,!(node_modules|__generated__|\\.next)/**}*.{mjs,js,json}'"
  },
  "license": "MIT",
  "dependencies": {
    "chance": "1.1.8",
    "graphql": "16.5.0",
    "next": "12.1.6",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-relay": "0.0.0-main-0a30fd6b",
    "relay-runtime": "0.0.0-main-0a30fd6b"
  },
  "devDependencies": {
    "autoprefixer": "10.4.7",
    "concurrently": "7.1.0",
    "postcss": "8.4.13",
    "prettier": "2.6.2",
    "relay-compiler": "0.0.0-main-9ff377ef",
    "tailwindcss": "3.0.24"
  },
  "prettier": {
    "bracketSameLine": true,
    "bracketSpacing": false,
    "singleQuote": true,
    "trailingComma": "all"
  }
}
