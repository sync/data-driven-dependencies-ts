{
  "name": "with-relay-modern",
  "version": "1.0.0",
  "description": "Example of Next.js with Relay Modern",
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "postinstall": "yarn deduplicate && patch-package",
    "deduplicate": "yarn yarn-deduplicate",
    "next": "next",
    "dev": "mkdir -p __generated__ && touch queryMap.json && node --require esbuild-register ./schema/printSchema.ts && concurrently \"next\" \"relay-compiler --watch\"",
    "build": "next build",
    "start": "next start",
    "relay": "relay-compiler",
    "schema": "node --require esbuild-register ./schema/printSchema.ts",
    "prettier": "prettier --write '{,!(node_modules|__generated__|\\.next)/**}*.{mjs,js,json}'",
    "typecheck": "tsc -b"
  },
  "license": "MIT",
  "dependencies": {
    "chance": "1.1.8",
    "graphql": "16.5.0",
    "next": "12.1.6",
    "nextjs-cors": "2.1.1",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-relay": "0.0.0-main-0a30fd6b",
    "relay-runtime": "0.0.0-main-0a30fd6b"
  },
  "devDependencies": {
    "@types/chance": "1.1.3",
    "@types/node": "17.0.31",
    "@types/react": "18.0.9",
    "@types/react-relay": "13.0.2",
    "@types/relay-runtime": "13.0.3",
    "autoprefixer": "10.4.7",
    "concurrently": "7.1.0",
    "esbuild": "0.14.38",
    "esbuild-register": "3.3.2",
    "patch-package": "6.4.7",
    "postcss": "8.4.13",
    "prettier": "2.6.2",
    "prettier-plugin-tailwindcss": "0.1.10",
    "relay-compiler": "0.0.0-main-9ff377ef",
    "tailwindcss": "3.0.24",
    "typescript": "4.6.4",
    "yarn-deduplicate": "5.0.0"
  },
  "prettier": {
    "bracketSameLine": true,
    "bracketSpacing": false,
    "singleQuote": true,
    "trailingComma": "all"
  }
}
